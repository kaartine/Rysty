        <form method="POST" action="login.action" name="loginform">
            <div align="center"><div style="width: 500px;"><div style="text-align: left;">

                <div class="form-header">
                    <p>
                        Enter your account details below to login to Confluence.
                    </p>
                </div>
                
                <div class="form-block">
                    <p>
                        <div class="steplabel" style="width: 150px;"><u>U</u>sername: </div><input type="text" name="os_username" tabindex="1" accesskey="U" size="30"><br/>
                    </p>
                    <p>
                        <div class="steplabel" style="width: 150px;"><u>P</u>assword: </div><input type="password" name="os_password" tabindex="2" accesskey="P" size="30"><br/>
                    </p>

                    <p>
                        <div class="steplabel" style="width: 150px;"><input type="checkbox" name="os_cookie" id="os_cookie_id" value="true" tabindex="3"/></div>
                        <label for="os_cookie_id" accesskey="r"><u>R</u>emember my login on this computer</label>
                    </p>
                </div>

                
                <div class="form-buttons">
                    <input id="loginButton" type="submit" value="Log In" tabindex="4"/>
                </div>

                <input type="hidden" name="os_destination" value="/spaces/createrssfeed.action?types=page"/>

                <div class="form-block">
                                    </div>

            </div></div></div>
        </form>

