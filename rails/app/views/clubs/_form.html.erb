<% form_for(@club) do |f| %>

<%= f.error_messages %>

<p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
</p>
<p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
</p>

<p>
    <%= f.label :founded %><br />
    <%= date_select("post", "founded", :start_year => 1800) %>
</p>
  
<p>  
<%= f.submit "Save" %>  
</p>

<% end %>
 